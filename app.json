{
  "expo": {
    "name": "FREED",
    "slug": "quit-nicotine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quitnicotine",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quitnicotine.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSPhotoLibraryUsageDescription": "Allow FREED to access your photos to set a profile picture."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.quitnicotine.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#22c55e"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow FREED to access your photos to set a profile picture."
        }
      ],
      "expo-localization",
      "expo-font",
      "react-native-audio-api"
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
